{
    "name":"VelociTclearer",
    "version":"1.0.0",
    "manifest_version": 2,
    "icons":{
        "128":"128.png",
        "48":"48.png",
        "16":"16.png"
    },
    "browser_action":{
        "default_icon":"16.png",
        "default_popup":"popup.html"
    },
    "content_security_policy":"script-src 'self' https://ajax.googleapis.com; object-src 'self'",
    "permissions": [
        "storage",
        "unlimitedStorage",
        "notifications",
        "contextMenus"
      ],
    "options_page": "options.html"

}